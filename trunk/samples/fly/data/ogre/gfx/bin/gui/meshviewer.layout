<?xml version="1.0" ?>
<GUILayout>
<Window Type="DefaultGUISheet" Name="root">
	
	<Window Type="TaharezLook/FrameWindow" Name="MeshViewer/MainWindow">
		<Property Name="Size" Value="w:0.25 h:1" />
		<Property Name="Position" Value="x:0.75 y:0" />
		<Property Name="DragMovingEnabled" Value="true" />
		<Property Name="RollUpEnabled" Value="true" />
		<Property Name="CloseButtonEnabled" Value="false" />
		<Property Name="Text" Value="Mesh Viewer" />

		<Window Type="TaharezLook/Combobox" Name="MeshViewer/MainWindow/MeshBox">
			<Property Name="Position" Value="x:0.05 y:0.05" />
			<Property Name="Size" Value="w:0.9 h:0.3" />
			<Property Name="Text" Value="Select Mesh" />
			<Property Name="ClippedByParent" Value="false" />
			<Property Name="ReadOnly" Value="true" />			
		</Window>

		<Window Type="TaharezLook/TabControl" Name="MeshViewer/MainTabs">
			<Property Name="Position" Value="x:0.05 y:0.1" />
			<Property Name="Size" Value="w:0.9 h:0.8" />
			
			<Window Type="DefaultGUISheet" Name="MeshViewer/MainTabs/AnimPage">
				<Property Name="Text" Value="Anims" />
			
				<Window Type="TaharezLook/StaticText" Name="MeshViewer/MainWindow/Text1">
					<Property Name="Position" Value="x:0.05 y:0.02" />
					<Property Name="Size" Value="w:0.9 h:0.04" />
					<Property Name="Text" Value="Animation States" />
					<Property Name="FrameEnabled" Value="false" />
				</Window>
				
				<Window Type="TaharezLook/Listbox" Name="MeshViewer/MainWindow/AnimList">
					<Property Name="Position" Value="x:0.05 y:0.08" />
					<Property Name="RelativeMaxSize" Value="w:1 h:1" />
					<Property Name="Size" Value="w:0.9 h:0.3" />
				</Window>
				
				<Window Type="TaharezLook/Button" Name="MeshViewer/MainWindow/AnimPlay">
					<Property Name="Position" Value="x:0.05 y:0.4" />
					<Property Name="Size" Value="w:0.4 h:0.04" />
					<Property Name="Text" Value="Play" />
				</Window>
				
				<Window Type="TaharezLook/Button" Name="MeshViewer/MainWindow/AnimStop">
					<Property Name="Position" Value="x:0.5 y:0.4" />
					<Property Name="Size" Value="w:0.4 h:0.04" />
					<Property Name="Text" Value="Stop" />
				</Window>
				
				<Window Type="TaharezLook/StaticText" Name="MeshViewer/MainWindow/Text2">
					<Property Name="Position" Value="x:0.05 y:0.46" />
					<Property Name="Size" Value="w:0.9 h:0.04" />
					<Property Name="Text" Value="Animation Speed" />			
					<Property Name="FrameEnabled" Value="false" />
				</Window>
						
				<Window Type="TaharezLook/HorizontalScrollbar" Name="MeshViewer/MainWindow/AnimSpeed">
					<Property Name="Position" Value="x:0.05 y:0.51" />
					<Property Name="Size" Value="w:0.9 h:0.02" />
					<Property Name="DocumentSize" Value="10" />
					<Property Name="ScrollPosition" Value="5" />
					<Property Name="PageSize" Value="2" />
					<Property Name="StepSize" Value="1" />
					<Property Name="OverlapSize" Value="1" />
				</Window>
				
				<Window Type="TaharezLook/Button" Name="MeshViewer/MainWindow/SpeedReset">
					<Property Name="Position" Value="x:0.05 y:0.54" />
					<Property Name="Size" Value="w:0.9 h:0.04" />
					<Property Name="Text" Value="Reset" />
				</Window>
				
				<Window Type="TaharezLook/MultiLineEditbox" Name="MeshViewer/MainWindow/Text3">
					<Property Name="Position" Value="x:0.05 y:0.6" />
					<Property Name="Size" Value="w:0.9 h:0.18" />
					<Property Name="Text" Value="Drag to rotate. Right-button drag to move." />
					<Property Name="ReadOnly" Value="true" />
				</Window>
			</Window>
		</Window>
		
		<Window Type="TaharezLook/Button" Name="MeshViewer/MainWindow/Exit">
			<Property Name="Position" Value="x:0.05 y:0.91" />
			<Property Name="Size" Value="w:0.9 h:0.04" />
			<Property Name="Text" Value="Exit" />
		</Window>
	</Window>
</Window>
</GUILayout>
